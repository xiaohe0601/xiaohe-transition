{
  "name": "xiaohe-transition",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@8.14.0",
  "description": "🏀 一个简单易用的贝塞尔曲线过渡动画工具",
  "author": "xiaohe0601 <HeDianGeng0601@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/xiaohe0601",
  "homepage": "https://github.com/xiaohe0601/xiaohe-transition#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaohe0601/xiaohe-transition.git"
  },
  "bugs": "https://github.com/xiaohe0601/xiaohe-transition/issues",
  "keywords": [
    "xiaohe",
    "bezier",
    "transition",
    "animation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "esno src/index.ts",
    "build": "nr build:step-1 && nr build:step-2",
    "build:step-1": "unbuild",
    "build:step-2": "npx babel dist --config-file ./src/.babelrc.json --out-dir lib --copy-files --keep-file-extension",
    "release": "nr build && bumpp && npm publish",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:type": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "xiaohe-nano-emitter": "^0.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@types/node": "^20.11.3",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
